{
  "source": "./app",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-flow-type-plugin",
      "option": {
        "enable": true
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    },
    {
      "name": "esdoc-standard-plugin"
    },
    {
      "name": "esdoc-jsx-plugin",
      "option": {
        "enable": true
      }
    }
  ]
}
