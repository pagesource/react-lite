  "scripts": {
    "start": "webpack-dev-server --open --config=./config/webpack.config.dev.js",
    "test": "cross-env NODE_ENV=test jest --config ./config/jest.config.js",
    "test:updateSnapshot": "yarn run test --updateSnapshot",
    "test:watch": "yarn run test --watch",
    "generate": "plop --plopfile generators/index.js",
    "storybook": "start-storybook -s ./app/components -p 6009",
    "build-storybook": "build-storybook -c .storybook -o ./out",
    "lint:js": "eslint app/",
    "lint:js:fix": "yarn run lint:js --fix",
    "lint:css": "stylelint app/**/*.js",
    "lint:css:fix": "yarn run lint:css --fix",
    "lint": "yarn run lint:js && yarn run lint:css",
    "flow": "flow",
    "flow:install": "flow-typed install",
    "flow:coverage": "flow-coverage-report -i 'app/**/*.js' -x 'app/**/*.spec.js' -t html -t json -t text -o reports/flow/ --threshold 70",
    "esdoc": "esdoc -c .esdoc.json",
    "pretty": "prettier --write **/*.{js,json,css}",
    "audit": "yarn run build -- --report",
    "clean:dist": "rimraf dist"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "jestSonar": {
    "reportPath": "reports/tests",
    "reportFile": "test-reporter.xml",
    "indent": 2
  },
  "resolutions": {
    "eslint-scope": "3.7.1"
  }
