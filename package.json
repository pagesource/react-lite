{
  "name": "react-lite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "cross-env NODE_ENV=test jest --config ./config/jest/jest.config.js",
    "test:updateSnapshot": "yarn run test --updateSnapshot",
    "test:watch": "yarn run test --watch",
    "lint:js": "eslint src/**/*.js",
    "lint:js:fix": "yarn run lint:js --fix",
    "lint:css": "stylelint src/**/*.js",
    "lint:css:fix": "yarn run lint:css --fix",
    "lint": "yarn run lint:js && yarn run lint:css",
    "flow": "flow",
    "flow:install": "flow-typed install",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js' -x 'src/**/*.spec.js' -t html -t json -t text -o reports/flow/ --threshold 70",
    "generate": "plop --plopfile generators/index.js",
    "storybook": "start-storybook -s ./src/static -p 6009",
    "build-storybook": "build-storybook -c .storybook -o ./out",
    "esdoc": "esdoc -c .esdoc.json",
    "pretty": "prettier --write '**/*.{js,json,css}'",
    "mock": "node ./canned.config.js"
  },
